{
    "name": "react-tally-counter",
    "version": "0.0.1",
    "license": "UNLICENSED",
    "private": false,
    "author": "mcannon",
    "repository": {
        "type": "git",
        "url": "https://github.com/matthewcannon/react-tally-counter.git"
    },
    "homepage": "https://github.com/matthewcannon/react-tally-counter#readme",
    "scripts": {
        "babel": "babel ./src --out-dir ./lib",
        "build": "npm run prettier && npm run jest && npm run babel && npm run pack && npm run sass && npm run copy-images",
        "clean": "rm -rf ./lib && rm -rf ./public && rm -rf ./coverage && rm -rf .cache",
        "copy-images": "mkdir -p ./public/image/ && cp ./src/image/* ./public/image/",
        "jest": "jest ./test/*.spec.js --coverage",
        "lint": "eslint --ext .js --ext .jsx ./src test",
        "pack": "webpack --mode development --entry ./src/index.jsx --output ./public/bundle.js",
        "prettier": "prettier --write {./src/**/*,./test/**/*,*}.{js,jsx,json}",
        "rebuild": "npm run clean && npm run build",
        "sass": "node-sass ./src/style/ --output-style compressed --output ./public/",
        "serve": "live-server ./public",
        "start": "npm run clean && npm run build && npm run serve",
        "test": "npm run prettier && npm run jest && npm run lint",
        "watch": "onchange -i './test/**/*.js' './src/**/*.js' './src/**/*.jsx' -- npm run build"
    },
    "dependencies": {
        "@babel/polyfill": "^7.0.0",
        "@babel/runtime": "^7.1.5",
        "ramda": "^0.26.1",
        "react": "0.14.7",
        "react-tween-state": "^0.1.5"
    },
    "devDependencies": {
        "@babel/cli": "7.1.2",
        "@babel/core": "^7.1.2",
        "@babel/plugin-transform-modules-commonjs": "^7.1.0",
        "@babel/plugin-transform-runtime": "^7.1.0",
        "@babel/preset-env": "^7.1.0",
        "@babel/preset-react": "^7.0.0",
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^23.6.0",
        "babel-loader": "^8.0.4",
        "babel-plugin-add-module-exports": "^1.0.0",
        "enzyme": "^3.7.0",
        "enzyme-adapter-react-14": "^1.2.0",
        "eslint": "^5.8.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^3.1.0",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jsx-a11y": "^6.1.2",
        "eslint-plugin-prettier": "^3.0.0",
        "eslint-plugin-react": "^7.11.1",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^23.6.0",
        "jest-enzyme": "^7.0.1",
        "jest-vim-reporter": "0.0.1",
        "live-server": "^1.2.0",
        "node-sass": "^4.10.0",
        "onchange": "^5.1.0",
        "prettier": "^1.14.3",
        "react-addons-test-utils": "0.14.7",
        "react-dom": "0.14.7",
        "webpack": "^4.26.0",
        "webpack-cli": "^3.1.2"
    }
}
